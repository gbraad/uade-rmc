CC = {CC}
CFLAGS = -Wall -O2 -g {CFLAGS}
LDFLAGS = {LDFLAGS}
PREFIX = {PREFIX}

RMCMODULES = rmc.o utils.o

all:	rmc

rmc:	$(RMCMODULES)
	$(CC) $(LDFLAGS) -o $@ $(RMCMODULES) -luade -lbencodetools

rmc.o:	rmc.c utils.h
utils.o:	utils.c utils.h

%.o:	%.c
	$(CC) $(CFLAGS) -c $<

clean:	
	rm -f rmc *.o
